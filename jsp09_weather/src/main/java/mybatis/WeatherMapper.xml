<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="WeatherMapper">
	<insert id="insert">
		INSERT INTO weather(tmFc,stnId,wfSv1,wn,wr)
		VALUES(#{tmFc},#{stnId},#{wfSv1},#{wn},#{wr})
	</insert>

	<select id="selectList" resultType="dto.Weather">
		SELECT * 
		FROM weather
		WHERE tmFc = #{tmFc} AND stnId = #{stnId}
	</select>
	
	<select id="List" resultType="dto.Weather">
		SELECT *
		FROM weather
		WHERE stnId = #{stnId}
		ORDER BY tmFc DESC
	</select>
	
	<select id="all" resultType="dto.Weather">
		SELECT *
		FROM weather
		ORDER BY tmFc DESC
	</select>
</mapper>